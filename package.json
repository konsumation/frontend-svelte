{
  "name": "konsum-frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "svelte web frontend for konsum",
  "keywords": [
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c",
    "start": "rollup -c -w",
    "package": "npm-pkgbuild pkgbuild pacman makepkg"
  },
  "dependencies": {
    "mf-styling": "arlac77/mf-styling",
    "svelte-common": "^2.0.3",
    "svelte-guard-history-router": "^2.2.1",
    "svelte-session-manager": "^1.1.4",
    "svelte-time-series": "arlac77/svelte-time-series"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "npm-pkgbuild": "^6.12.1",
    "postcss-import": "^12.0.1",
    "rollup": "^2.18.2",
    "rollup-plugin-consts": "^1.0.2",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svelte": "git+https://github.com/ctavan/rollup-plugin-svelte.git#handle-missing-exports-gracefully",
    "rollup-plugin-terser": "^6.1.0",
    "semantic-release": "^17.1.1",
    "svelte": "^3.23.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/konsum-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/konsum-frontend-svelte/issues"
  },
  "homepage": "https://github.com/arlac77/konsum-frontend-svelte#readme",
  "config": {
    "api": "/services/konsum/api",
    "base": "/services/konsum",
    "proxyTarget": "https://mfelten.dynv6.net"
  },
  "pacman": {
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.19.0",
      "konsum": ">=1.34.37"
    },
    "groups": "home automation",
    "hooks": "pacman/hooks.sh",
    "installdir": "/services/konsum/frontend"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --pkgver ${nextRelease.version} pkgbuild pacman makepkg"
        }
      ]
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-app"
    ]
  }
}
